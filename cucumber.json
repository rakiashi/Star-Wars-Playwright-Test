{
    "default":{
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "dryRun": false,
        "paths":["playwright/test/features"],
        "require": [
            "playwright/test/steps/*.ts",
            "playwright/helpers/hook.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "rerun:test-results/rerun/@rerun.txt"
        ],
        "parallel":1
    },
    "rerun":{
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "dryRun": false,
        "require": [
            "playwright/test/steps/*.ts",
            "playwright/helpers/hook.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html"
        ],
        "parallel":1
    }
}